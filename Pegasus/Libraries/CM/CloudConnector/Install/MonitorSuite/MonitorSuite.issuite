		<SetProperty Id="DetectConfigurationFile" Name="ConfigurationFileExist" Value="true"/>
		<CallInstallScript Id="SetShowConnectorEndpoint" Arguments="SetShowConnectorEndpoint" ExecutionLevel="Admin"/>
		<CallInstallScript Id="RemoveShowConnectorEndpoint" Arguments="RemoveShowConnectorEndpoint" ExecutionLevel="Admin"/>
		<CallInstallScript Id="CheckForUpgrade" Arguments="CheckRunConfig"/>
		<SetProperty Id="CheckSupportedPlatform" Name="UnsupportedPlatform" Value="true"/>
		<CallInstallScript Id="StartServices" Arguments="StartServices"/>
		<SetProperty Id="CheckPendingReboot" Name="PendingReboot" Value="true"/>
		<SetProperty Id="SetUpgradeDotNet" Name="RunDotNet" Value="true"/>
		<SetProperty Id="CheckModernOS" Name="ModernOS" Value="true"/>
		<CallInstallScript Id="GetPrevInstallDir" ExecutionLevel="Admin" Arguments="GetPrevInstallDir"/>
		<SetProperty Id="SetUpgradeInstallDir" Name="ISInstallDir_Sage_Data_Cloud_Connector" Value="[PrevInstallDir]" FormatPropertyValue="Yes"/>
