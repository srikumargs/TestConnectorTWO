		<CallInstallScript Id="CheckIfAdmin" Arguments="CheckIfAdmin"/>
		<SetProperty Id="SetNonProdFlag"/>
	</Actions>
	<Events>
		<OnEnd>
			<Action Id="LaunchMonitor">
				<When>
					<All>
						<FileExists Path="[ISInstallDir_Sage_Data_Cloud_Connector]\ConnectorServiceMonitor.exe"/>
						<PropertyValue Name="ISSilentInstall" Convert="String" Compare="NotEqual" CompareTo="true"/>
					</All>
				</When>
			</Action>
		</OnEnd>
		<OnBegin>
			<Action Id="CheckIfAdmin"/>
			<Action Id="DetermineUpgrade"/>
			<Action Id="CheckSupportedPlatform">
				<When>
					<Any>
						<Platform OSVersion="-6.0" ProductType="Workstation,Server,DomainController"/>
					</Any>
				</When>
			</Action>
			<Action Id="CheckPendingReboot">
				<When>
					<Any>
						<RegistryExists Check64Bit="False" Key="HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Component Based Servicing\RebootPending"/>
						<RegistryExists Check64Bit="False" Key="HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\WindowsUpdate\Auto Update\RebootRequired"/>
						<RegistryExists Check64Bit="False" Key="HKLM\System\CurrentControlSet\Control\Session Manager\PendingFileRenameOperations"/>
					</Any>
				</When>
			</Action>
			<Action Id="SetUpgradeDotNet">
				<When>
					<All>
						<RegistryExists Check64Bit="False" Key="HKLM\SOFTWARE\Microsoft\NET Framework Setup\NDP\v4\Full"/>
						<Not>
							<RegistryValue Check64Bit="False" Key="HKLM\SOFTWARE\Microsoft\NET Framework Setup\NDP\v4\Full" ValueName="Release" Convert="String" Compare="GreaterEqual" CompareTo="378675"/>
						</Not>
					</All>
				</When>
			</Action>
			<Action Id="CheckModernOS">
				<When>
					<All>
						<Platform OSVersion="6.2-"/>
					</All>
				</When>
			</Action>
			<Action Id="CheckForUpgrade"/>
			<Action Id="GetPrevInstallDir">
				<When>
					<Any>
						<MsiRelated UpgradeCode="{A53A6887-79CA-49B5-B35D-6492C66E3554}"/>
					</Any>
				</When>
			</Action>
			<Action Id="SetUpgradeInstallDir">
				<When>
					<Any>
						<PropertyValue Name="PrevInstallDir" Convert="String" Compare="NotEqual" CompareTo="NA"/>
					</Any>
				</When>
			</Action>
		</OnBegin>
		<OnParcelsInstalling>
			<Action Id="StopServices"/>
		</OnParcelsInstalling>
		<OnParcelsInstalled>
			<Action Id="StartServices">
				<When>
					<Any>
						<FeatureAction FeatureName="PegasusConnector" State="install"/>
					</Any>
				</When>
			</Action>
			<Action Id="DetectMonitorFile">
				<When>
					<All>
						<FileExists Path="[ISInstallDir_Sage_Data_Cloud_Connector Monitor]\Sage.CRE.HostingFramework.ServiceMonitor.exe"/>
					</All>
				</When>
			</Action>
		</OnParcelsInstalled>
	</Events>
	<AbortConditions>
		<Message Text="IDS_SUITE_MINOR_DOWNGRADE">
			<When>
				<Any>
					<Not>
						<ParcelRef Id="*"/>
					</Not>
					<SuiteInstalled SuiteId="*" Version="*" Compare="GreaterThan"/>
				</Any>
			</When>
		</Message>
		<Message Text="ID_STRING63">
			<When>
				<Not>
					<Any>
						<ParcelRef Id="{B7E2BD54-E2FF-4B38-A5A9-BA8C70BB54B3}"/>
						<ParcelRef Id="{E5EB8F74-C236-4FB5-B97C-2994BFB45648}"/>
					</Any>
				</Not>
			</When>
		</Message>
	</AbortConditions>
