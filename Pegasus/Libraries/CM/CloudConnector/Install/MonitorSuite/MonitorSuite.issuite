		<CallInstallScript Id="CheckForNephos" Arguments="CheckForNephos" ExecutionLevel="Admin"/>
		<SetProperty Id="CheckForUpgradeCase" Name="STARTCONNECTORCONFIG" Value="False"/>
		<CallInstallScript Id="DebugStuff" Arguments="DebugStuff"/>
		<CallInstallScript Id="CheckRunConfig" Arguments="CheckRunConfig"/>
		<StartExe Id="LaunchMonitor" DefaultCode="Ignore" File="[ISInstallDir_Sage_Data_Cloud_Connector Monitor]\ConnectorServiceMonitor.exe" ExecutionLevel="Admin"/>
